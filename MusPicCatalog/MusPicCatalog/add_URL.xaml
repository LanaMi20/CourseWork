        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MusPicCatalog"
        mc:Ignorable="d"
        Title="add_URL" Height="450" Width="800">
    <Grid>
        <Button x:Name="OK" Content="Button" HorizontalAlignment="Left" Margin="260,345,0,0" VerticalAlignment="Top" Click="OK_Click"/>
        <Button x:Name="cancel" Content="Button" HorizontalAlignment="Left" Margin="469,350,0,0" VerticalAlignment="Top" Click="cancel_Click"/>
        <TextBox x:Name="name" HorizontalAlignment="Left" Margin="152,188,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="url" HorizontalAlignment="Left" Margin="289,103,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="categr" HorizontalAlignment="Left" Margin="572,206,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
